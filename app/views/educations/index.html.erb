<p id="notice"><%= notice %></p>

<h1>Listing Education </h1>

<table>
  <thead>
    <tr>
      <th>Degree</th>
      <th>institution</th>
      <th>description</th>
      <th>year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educations.each do |education| %>
      <tr>
        <td><%= education.degree %></td>
        <td><%= education.institution %></td>
        <td><%= education.desc %></td>
        <td><%= education.year %></td>
        <td><%= link_to 'Show', education %></td>
        <td><%= link_to 'Edit', edit_education_path(education) %></td>
        <td><%= link_to 'Destroy', education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Educaiton', new_education_path %>
